{
  "entry": {
    "vue-simulator-renderer": "src/index"
  },
  "sourceMap": true,
  "library": "___VueSimulatorRenderer___",
  "libraryTarget": "umd",
  "externals": {
    "vue": "var window.Vue",
    "vue-router": "var window.VueRouter",
    "moment": "var window.moment",
    "lodash": "var window._"
  },
  "polyfill": false,
  "outputDir": "dist",
  "vendor": false,
  "ignoreHtmlTemplate": true,
  "plugins": [
    ["build-plugin-react-app"],
    ["build-plugin-moment-locales", { "locales": ["zh-cn"] }],
    "./build.plugin.js"
  ]
}
